<main>
    <h2>Sign In!</h2>
    <form action="<%= session_url %>" method="post">
        <%= auth_token %>
        <label for="username">Username: </label>
        <input type="text" id="username" name="session[username]"><br><br>
        <label for="password">Password: </label>
        <input type="password" id="password" name="session[password]"><br><br>
        <input type="submit" value="Log In">
    </form>
</main>